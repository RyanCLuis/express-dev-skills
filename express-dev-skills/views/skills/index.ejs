<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <title>Dev Skills</title>
</head>
<body>
    <a href="/">Home</a>
    <h1>Skills</h1>
    <ul>
        <% skills.forEach(function(s) { %>
            <li>
                <%= s.skill %>
                -
                <%= s.helpful ? 'Yes' : 'No' %>
            </li>
        <% }) %>
    </ul>
</body>
</html>